#include <stdio.h>

void fibonacci(int n) {
    long long first = 0, second = 1, next;

    printf("Fibonacci sequence up to %d terms: \n", n);
    
    for(int i = 1; i <= n; i++) {
        if(i == 1) {
            printf("%lld ", first);
        } else if(i == 2) {
            printf("%lld ", second);
        } else {
            next = first + second;
            first = second;
            second = next;
            printf("%lld ", next);
        }
    }
    printf("\n");
}

int main() {
    int n;

    // Ask the user for the number of terms
    printf("Enter the number of terms: ");
    scanf("%d", &n);

    // Call the fibonacci function
    fibonacci(n);

    return 0;
}
